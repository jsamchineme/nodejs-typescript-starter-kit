{
  "name": "nodejs-typescript-starter-kit",
  "version": "0.0.1",
  "engines": {
    "node": "12.7.0"
  },
  "description": "boilerplace for nodejs typescript api",
  "author": "@jsamchineme",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "ts-node -r tsconfig-paths/register src/server.ts",
    "start:dev": "concurrently --handle-input \"wait-on dist/server.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "link-module-alias && node dist/server.js",
    "lint": "tslint -p tsconfig.json -c tslint.json"
  },
  "_moduleAliases": {
    "src": "dist"
  },
  "dependencies": {
    "@types/dotenv": "^6.1.1",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "http": "0.0.0",
    "jsonwebtoken": "^8.5.1",
    "link-module-alias": "^1.2.0",
    "rimraf": "2.6.3",
    "rxjs": "6.5.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/express": "4.17.0",
    "@types/node": "12.6.8",
    "concurrently": "4.1.1",
    "nodemon": "1.19.1",
    "ts-node": "8.3.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.18.0",
    "typescript": "3.5.3",
    "wait-on": "3.3.0"
  }
}
